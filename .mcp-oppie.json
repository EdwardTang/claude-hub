{
	"mcpServers": {
		"task-master-ai": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"--package=task-master-ai",
				"task-master-ai"
			],
			"env": {
				"ANTHROPIC_API_KEY": "your_key_here",
				"PERPLEXITY_API_KEY": "your_key_here",
				"OPENAI_API_KEY": "your_key_here",
				"GOOGLE_API_KEY": "your_key_here",
				"OPENROUTER_API_KEY": "your_key_here"
			},
			"description": "Task Master AI"
		},
		"serena": {
			"type": "stdio",
			"command": "uv",
			"args": [
				"run",
				"--directory",
				"/path/to/serena",
				"serena-mcp-server",
				"--context",
				"ide-assistant"
			]
		},
		"mcp-deepwiki-local": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"mcp-deepwiki@latest"
			],
			"env": {},
			"description": "Fetch a deepwiki repo (Local npm package version)"
		},
		"devin-deepwiki": {
			"type": "stdio",
			"env": {},
			"description": "Official Devin.ai DeepWiki MCP server - AI-powered GitHub repository documentation and search",
			"url": "https://mcp.deepwiki.com/sse"
		},
		"git": {
			"type": "stdio",
			"command": "node",
			"args": [
				"/path/to/git-mcp-server/dist/index.js"
			],
			"env": {},
			"description": ""
		},
		"context7-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@smithery/cli@latest",
				"run",
				"@upstash/context7-mcp",
				"--key",
				"your_key_here"
			],
			"env": {},
			"description": "Context7 MCP server for real-time documentation"
		},
		"sequential-thinking": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"env": {},
			"description": ""
		},
		"exa": {
			"type": "stdio",
			"command": "node",
			"args": [
				"/path/to/exa-mcp-server/build/index.js"
			],
			"env": {
				"EXA_API_KEY": "your_key_here"
			},
			"description": "Exa AI Search API for real-time web searches"
		}
	}
}